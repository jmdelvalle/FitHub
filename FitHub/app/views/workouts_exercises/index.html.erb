<p id="notice"><%= notice %></p>

<h1>Listing Workouts Exercises</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workouts_exercises.each do |workouts_exercise| %>
      <tr>
        <td><%= link_to 'Show', workouts_exercise %></td>
        <td><%= link_to 'Edit', edit_workouts_exercise_path(workouts_exercise) %></td>
        <td><%= link_to 'Destroy', workouts_exercise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workouts exercise', new_workouts_exercise_path %>
