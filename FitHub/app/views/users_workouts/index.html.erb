<p id="notice"><%= notice %></p>

<h1>Listing Users Workouts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_workouts.each do |users_workout| %>
      <tr>
        <td><%= link_to 'Show', users_workout %></td>
        <td><%= link_to 'Edit', edit_users_workout_path(users_workout) %></td>
        <td><%= link_to 'Destroy', users_workout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

