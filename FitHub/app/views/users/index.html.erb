<p id="notice"><%= notice %></p>

<center><h1>Everyone On FitHub</h1></center>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user= User.all %>
    <% @user.select do |user| %>
    <% if user.name != current_user.name%>
    <div class="container-fluid" id="medium_size">
      <div class="well well-sm">
          <center>
            <h4><%= user.name %><br><small><%= link_to "Add Friend", friendships_path(:friend_id => user), :method => :post %></small></h4>
          </center>
      </div>
    </div>


    <tr>

    </tr><br>
    <% end %>
    <% end %>
  </tbody>
</table>

<br>
